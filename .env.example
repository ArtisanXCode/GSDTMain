# Add your environment variables here
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Currency Configuration
VITE_BASKET_CURRENCIES=CNY,THB,INR,BRL,ZAR,IDR,USD
VITE_REFERENCE_CURRENCIES=USD

# Currency Colors (JSON format)
VITE_CURRENCY_COLORS={"CNY":"#ed9030","BRL":"#ed9030","ZAR":"#ed9030","THB":"#ed9030","INR":"#ed9030","IDR":"#ed9030","USD":"#ed9030"}

# Currency Display Order for compact view (subset of basket currencies)
VITE_COMPACT_CURRENCIES=CNY,THB,INR,BRL

# Currency Decimal Precision Configuration (JSON format)
VITE_CURRENCY_PRECISION={"USD":4,"IDR":4,"INR":4,"THB":4,"ZAR":4,"CNY":4,"BRL":4}

# Currency Symbols (JSON format)
VITE_CURRENCY_SYMBOLS={"USD":"$","CNY":"¥","CNH":"¥","BRL":"R$","INR":"₹","ZAR":"R","IDR":"Rp","THB":"฿","JPY":"¥","EUR":"€","CAD":"C$"}

# Currency Names (JSON format)
VITE_CURRENCY_NAMES={"USD":"US Dollar","CNY":"Chinese Yuan","CNH":"Chinese Yuan","BRL":"Brazilian Real","INR":"Indian Rupee","ZAR":"South African Rand","IDR":"Indonesian Rupiah","THB":"Thai Baht","JPY":"Japanese Yen","EUR":"Euro","CAD":"Canadian Dollar"}

# Exchange Rate API
VITE_EXCHANGE_RATE_API_URL=https://api.exchangerate-api.com/v4/latest

VITE_BSC_SCAN_API_KEY=your_bscscan_api_key
VITE_BSC_SCAN_API_LINK=https://api-testnet.bscscan.com/
VITE_BSC_SCAN_EXPLORER_LINK=https://testnet.bscscan.com/

# Sumsub API
VITE_SUMSUB_APP_TOKEN=your_sumsub_app_token
VITE_SUMSUB_SECRET_KEY=your_sumsub_secret_key
VITE_SUMSUB_WEBHOOK_SECRET=your_sumsub_webhook_secret

# Blockchain
VITE_CONTRACT_ADDRESS=0x9E3deC5B983Ca20d7Ba64b12db48738585f6E00a
VITE_GSDC_DECIMALS=18

# NowPayments API
VITE_NOWPAYMENTS_API_KEY=your_nowpayments_api_key

# API Key
VITE_API_KEY=your_api_key

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# BSCScan API key for contract verification
BSCSCAN_API_KEY=your_bscscan_api_key_here

# BSC Testnet RPC URL (optional, fallback in hardhat config)
BSC_TESTNET_RPC=https://data-seed-prebsc-1-s1.bnbchain.org:8545